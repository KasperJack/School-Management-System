    background-color: #5e81ac; /* Nord9 - Blue for checked state */
    border: 1px solid #5e81ac; /* Matching border color */
    border-radius: 4px;
}

/* Unchecked state */
QCheckBox::indicator:unchecked {
    background-color: #f0f0f0; /* Light background for unchecked */
    border: 1px solid #cccccc; /* Light gray border */
    border-radius: 4px;
}
