     <string notr="true">QScrollArea {
    border: 1px solid #3b4252; /* Nord1 - Dark gray-blue border */
    background: #f9f9f9; /* Keep original background */
    border-radius: 0px;
}

/* Scroll Bar */
QScrollBar:vertical {
    border: none;
    background: #e6e6e6; /* Keep original background */
    width: 12px;
    margin: 2px;
    border-radius: 6px;
}

QScrollBar::handle:vertical {
    background: #4c566a; /* Nord3 - Medium gray-blue for handle */
    min-height: 20px;
    border-radius: 6px;
}

QScrollBar::handle:vertical:hover {
    background: #81a1c1; /* Nord8 - Blue on hover */
}

QScrollBar::sub-line:vertical,
QScrollBar::add-line:vertical {
    background: none;
    height: 0;
}

/* Items inside the Scroll Area */
QWidget {
    font-size: 16px;
    font-family: Arial, sans-serif;
    color: #2e3440;
    background: transparent;
}

QCheckBox {
    spacing: 10px;
    padding: 5px;
}

QCheckBox::indicator {
    width: 16px;
    height: 16px;
}

/* Checked state */
QCheckBox::indicator:checked {
    background-color: #5e81ac; /* Nord9 - Blue for checked state */
    border: 1px solid #5e81ac; /* Matching border color */
    border-radius: 4px;
}

/* Unchecked state */
QCheckBox::indicator:unchecked {
    background-color: #f0f0f0; /* Light background for unchecked */
    border: 1px solid #cccccc; /* Light gray border */
    border-radius: 4px;
}
/* RadioButton Styling */
QRadioButton {
    spacing: 6px; /* Space between the radio button and label text */
    color: #2e3440;/* Light grayish blue text from Nord */
    font-size: 11pt;
}

/* RadioButton Indicator */
QRadioButton::indicator {
    width: 14px;
    height: 14px;
    border-radius: 9px; /* Circular indicator */
    border: 2px solid #5e81ac; /* Nord9 Blue for the border */
    background-color: transparent; /* Transparent background */
}

/* Hover State for Indicator */
QRadioButton::indicator:hover {
    border-color: #88c0d0; /* Nord7 Lighter Teal on hover */
}

/* Checked State for Indicator */
QRadioButton::indicator:checked {
    background-color: #81a1c1; /* Nord8 Blue when checked */
    border-color: #81a1c1; /* Matching border color */
}

/* Pressed State for Indicator */
QRadioButton::indicator:pressed {
    background-color: #5e81ac; /* Darker blue on press */
}

/* Hover State for Checked Indicator */
QRadioButton::indicator:checked:hover {
    background-color: #88c0d0; /* Nord7 Lighter Teal on hover when checked */
}

/* Disabled State for Indicator */
QRadioButton::indicator:disabled {
    border-color: #3b4252; /* Nord1 Darker gray for disabled state */
    background-color: #4c566a; /* Nord3 Dark gray background for disabled */
}

/* Checked Disabled State for Indicator */
QRadioButton::indicator:checked:disabled {
    background-color: #4c566a; /* Dark gray checked state for disabled */
    border-color: #4c566a; /* Dark gray border for disabled checked state */
}

