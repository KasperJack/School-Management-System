/* Scroll Bar */
QScrollBar:vertical {
    border: none;
    background: #e6e6e6; /* Keep original background */
    width: 12px;
    margin: 2px;
    border-radius: 6px;
}

QScrollBar::handle:vertical {
    background: #4c566a; /* Nord3 - Medium gray-blue for handle */
    min-height: 20px;
    border-radius: 6px;
}

QScrollBar::handle:vertical:hover {
    background: #81a1c1; /* Nord8 - Blue on hover */
}

QScrollBar::sub-line:vertical,
QScrollBar::add-line:vertical {
    background: none;
    height: 0;
}

/* Items inside the Scroll Area */
QWidget {
    font-size: 16px;
    font-family: Arial, sans-serif;
    color: #2e3440;
    background: transparent;
}

QCheckBox {
    spacing: 10px;
    padding: 5px;
}

QCheckBox::indicator {
    width: 16px;
    height: 16px;
}

/* Checked state */
