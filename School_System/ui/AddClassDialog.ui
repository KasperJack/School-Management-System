    <string notr="true">/* Base ComboBox */
QComboBox {
    background-color: #3B4252; /* Darker background to fit the Nord theme */
    color: #ECEFF4; /* Light text */
    border: 1px solid #4C566A; /* Neutral gray-blue border */
    border-radius: 0px; /* No rounded corners */
    font-family: &quot;Segoe UI&quot;; /* System font */
    font-size: 15px; /* Standard font size */
    padding: 6px; /* Maintain padding */
}

/* Arrow Button */
QComboBox::drop-down {
    border: none; /* Remove border around the drop-down button */
    background-color: #434C5E; /* Slightly darker gray for the arrow background */
    border-left: 1px solid #4C566A; /* Subtle separator */
    width: 20px; /* Keep original width */
}

/* Drop-down Arrow */
QComboBox::down-arrow {
    image: url(down-arrow-icon.png); /* Custom arrow icon if needed */
    width: 10px;
    height: 10px;
}

/* Drop-down List (Popup Menu) */
QComboBox QAbstractItemView {
    background-color: #2E3440; /* Dark background for the list */
    color: #ECEFF4; /* Light text for readability */
    border: 1px solid #4C566A; /* Match the border to the base combo box */
    selection-background-color: #88C0D0; /* Teal highlight for selected items */
    selection-color: #2E3440; /* Dark text on selection for contrast */
}

/* Hover Effect */
QComboBox:hover {
    border: 1px solid #5E81AC; /* Light blue-gray border on hover */
}

/* Focused ComboBox */
QComboBox:focus {
    border: 1px solid #88C0D0; /* Teal border when focused */
}
