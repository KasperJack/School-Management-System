   <property name="styleSheet">
    <string notr="true">QTabBar::tab {
    background: #E5E5E5; /* Light gray background for unselected tabs */
    color: #000000; /* Black text color */
    border: 1px solid transparent; /* Transparent border to prevent outlines */
    border-bottom: none; /* Remove bottom border for seamless alignment with content */
    padding: 4px 12px; /* Padding for consistent size */
    min-width: 100px; /* Minimum tab width */
    border-top-left-radius: 5px; /* Rounded top-left corner */
    border-top-right-radius: 5px; /* Rounded top-right corner */
    margin-left: 0px; /* Adjust spacing between tabs */
    outline: none; /* Remove focus outline */
}

/* Selected Tab */
QTabBar::tab:selected {
    background: qlineargradient(
        spread:pad, x1:0, y1:0, x2:1, y2:1,
        stop:0 #0078D4, stop:1 #005A9E
    ); /* Gradient for selected tab */
    border: 1px solid transparent; /* Transparent border */
    color: white; /* White text for contrast */
    border-top-left-radius: 5px; /* Match rounded corners */
    border-top-right-radius: 5px;
    padding: 4px 12px; /* Match padding to ensure consistent size */
    outline: none; /* Remove focus outline */
}

/* Prevent Hover Effect on Selected Tab */
QTabBar::tab:hover:!selected {
    background: #F0F0F0; /* Light gray hover effect */
    color: #000000; /* Ensure text stays visible */
    border: 1px solid transparent; /* Transparent border to match other tabs */
}

/* Tab Bar Scroll Buttons (if tabs don't fit) */
QTabBar::scroller {
    border: none;
}

/* Tab Widget Panel */
QTabWidget::pane {
    border: 1px solid transparent; /* Transparent border for panel */
    background: #FFFFFF; /* White content area background */
    border-radius: 5px; /* Rounded corners */
    margin-top: -1px; /* Align pane with tabs */
    outline: none; /* Remove focus outline */
}

/* Center Align the Tab Bar */
QTabWidget::tab-bar {
    alignment: center; /* Center align the tabs */
}

/* Optional: Reduce Tab Height (Override default height) */
QTabBar::tab {
    height: 24px; /* Reduce the height of tabs */
}
