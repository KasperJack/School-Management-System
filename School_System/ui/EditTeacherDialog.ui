    margin-left: 0px; /* Adjust spacing between tabs */
    outline: none; /* Remove focus outline */
}

/* Selected Tab */
QTabBar::tab:selected {
    background: #8FBCBB; /* Nord teal color for selected tab */
    border: 1px solid #88C0D0; /* Slight border */
    color: #2E3440; /* Darker text for contrast */
    padding: 4px 12px; /* Match padding to ensure consistent size */
}

/* Prevent Hover Effect on Selected Tab */
QTabBar::tab:hover:!selected {
    background: #4C566A; /* Darker gray-blue hover effect */
    color: #D8DEE9; /* Ensure text stays visible */
