    border: 1px solid #d8dee9; /* Nord4 - Light gray border */
    padding: 6px;
    selection-background-color: #81a1c1; /* Nord8 - Light blue highlight */
    selection-color: white; /* White text color for selected text */
}

/* Hover State */
QDateEdit:hover {
    border: 1px solid #3b4252; /* Nord1 - Darker border on hover */
}

/* Focus State */
QDateEdit:focus {
    border: 1px solid #5e81ac; /* Nord9 - Blue border on focus */
    background-color: #ffffff; /* White background when focused */
}

/* Drop-down Calendar Button */
QDateEdit::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: center right;
    width: 24px; /* Width of the drop-down button */
    border-left: 1px solid #d8dee9; /* Light gray separator */
    border-top-right-radius: 6px; /* Match QDateEdit rounding */
    border-bottom-right-radius: 6px;
}

/* Drop-down Calendar Button Icon */
QDateEdit::down-arrow {
    image: url(:/newPrefix/more_icons/calendkar.png);
    width: 16px;
    height: 16px;
}

/* Drop-down Calendar Button Hover State */
QDateEdit::drop-down:hover {
    background-color: #d8dee9; /* Light gray background on hover */
}

/* Drop-down Calendar Button Pressed State */
QDateEdit::drop-down:pressed {
    background-color: #d0d4db; /* Slightly darker gray when pressed */
}

/* Drop-down Calendar Widget */
QCalendarWidget {
    background-color: #2e3440; /* Dark background */
    color: #d8dee9; /* Light text */
    border: 1px solid #4C566A; /* Subtle border */
    border-radius: 6px; /* Rounded corners */
}

/* Calendar Header (Month &amp; Year) */
QCalendarWidget QWidget#qt_calendar_navigationbar {
    background-color: #000000; /* Fully black header */
    color: #ffffff; /* White text */
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
}

/* Calendar Header Buttons (Previous/Next Month) */
QCalendarWidget QToolButton {
    background-color: #000000; /* Black background */
    color: white; /* White text */
    border-radius: 4px;
}

/* Calendar Header Hover */
QCalendarWidget QToolButton:hover {
    background-color: #5e81ac; /* Blue */
}

/* Calendar Grid (Days) */
QCalendarWidget QTableView {
    background-color: #2e3440; /* Dark grid background */
    color: #eceff4; /* Light text */
}

/* Selected Day */
QCalendarWidget QAbstractItemView:enabled:selected {
    background-color: #81a1c1; /* Light blue */
    color: white;
    border-radius: 4px;
}
