    border: none;
    background: #f3f3f3;
    width: 8px;
    margin: 0px 0px 0px 0px;
}
QScrollBar::handle:vertical {
    background: #cccccc;
    border-radius: 4px;
}
QScrollBar::handle:vertical:hover {
    background: #aaaaaa;
}
